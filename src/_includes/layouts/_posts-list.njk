<ol class="posts-list ui-list">
{% set posts = collections.posts %}
{% for post in posts %}
{# {{ post | dumpB | safe }} #}
  <li class="posts-list-el">
    <p class="meta-text"><time datetime="{{ post.date | htmlDateString }}">{{ post.date | readableDate }}</time> <span class="reading-time">{{ helpers.getReadingTime(content) }}m</span></p>
    
    {# {% for tag in post.data.tags %}
      <a href="/blog/tags/{{ tag | slug }}" class="tag">{{ tag }}</a>
    {% endfor %} #}
    <h1><a href="{{ post.url | url }}">{% if post.data.title %}{{ post.data.title | markdown | safe }}{% else %}<code>{{ post.url }}</code>{% endif %}</a></h1>
  </li>
{%endfor%}
</ol>

