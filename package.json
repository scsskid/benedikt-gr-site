{
  "name": "2020-benedikt",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "eleventy:default": "npx eleventy",
    "eleventy:watch": "npx eleventy --watch",
    "serve": "light-server -c .lightserverrc",
    "dev": "npm-run-all clean eleventy:default --parallel eleventy:watch serve --print-label",
    "dev:debug": "DEBUG=* npm run dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "light-server": "^2.6.4"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.9.0",
    "moment": "^2.24.0",
    "npm-run-all": "^4.1.5",
    "prettier-stylelint": "^0.4.2",
    "stylelint": "^11.1.1",
    "stylelint-config-standard": "^19.0.0"
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "./node_modules/prettier-stylelint/config.js"
    ],
    "rules": {
      "at-rule-no-unknown": [
        true,
        {
          "ignoreAtRules": [
            "mixin",
            "define-mixin",
            "media",
            "import",
            "custom-selector",
            "custom-media",
            "font-face",
            "import-sanitize"
          ]
        }
      ],
      "block-no-empty": null,
      "no-duplicate-selectors": null
    }
  }
}
